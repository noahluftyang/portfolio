{
  "name": "@portfolio/social-networking-server",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "npx prisma generate",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "develop": "nest start --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "7.1.1",
    "@nestjs/config": "0.5.0",
    "@nestjs/core": "7.1.1",
    "@nestjs/graphql": "7.4.1",
    "@nestjs/jwt": "7.0.0",
    "@nestjs/passport": "7.0.0",
    "@nestjs/platform-express": "7.1.1",
    "@nestjs/serve-static": "2.1.1",
    "@nestjs/swagger": "4.5.10",
    "@prisma/client": "2.0.0",
    "apollo-server": "2.14.5",
    "apollo-server-express": "2.14.5",
    "bcrypt": "5.0.0",
    "class-validator": "0.12.2",
    "firebase-admin": "8.12.1",
    "graphql": "15.1.0",
    "graphql-subscriptions": "1.1.0",
    "graphql-tools": "6.0.7",
    "helmet": "3.22.0",
    "nestjs-pino": "1.2.0",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.5.4",
    "swagger-ui-express": "4.1.4"
  },
  "devDependencies": {
    "@nestjs/cli": "7.2.0",
    "@nestjs/schematics": "7.0.1",
    "@nestjs/testing": "7.1.3",
    "supertest": "4.0.2",
    "ts-jest": "26.1.0",
    "ts-loader": "7.0.5",
    "tsconfig-paths": "3.9.0"
  },
  "engines": {
    "node": ">= 12"
  },
  "publishConfig": {
    "access": "public"
  }
}
